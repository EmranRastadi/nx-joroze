{
  "version": 2,
  "projects": {
    "nx-joroze": {
      "root": "apps/nx-joroze",
      "sourceRoot": "apps/nx-joroze",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/nx-joroze",
            "outputPath": "dist/apps/nx-joroze"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "nx-joroze:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "nx-joroze:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "nx-joroze:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nx-joroze"],
          "options": {
            "jestConfig": "apps/nx-joroze/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nx-joroze/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "nx-joroze-e2e": {
      "root": "apps/nx-joroze-e2e",
      "sourceRoot": "apps/nx-joroze-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-joroze-e2e/cypress.json",
            "tsConfig": "apps/nx-joroze-e2e/tsconfig.e2e.json",
            "devServerTarget": "nx-joroze:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-joroze:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/nx-joroze-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "nx-joroze"
}
